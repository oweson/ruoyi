            <if test="featuresDesc != null  and featuresDesc != ''"> and features_desc = #{featuresDesc}</if>
        </where>
    </select>
    
    <select id="selectGithubFeaturev1ById" parameterType="Long" resultMap="GithubFeaturev1Result">
