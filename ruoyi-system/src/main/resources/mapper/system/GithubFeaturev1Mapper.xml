    <delete id="deleteGithubFeaturev1ById" parameterType="Long">
        delete from github_featurev1 where id = #{id}
    </delete>

    <delete id="deleteGithubFeaturev1ByIds" parameterType="String">
        delete from github_featurev1 where id in 
