    <select id="selectGithubFeaturev1List" parameterType="GithubFeaturev1" resultMap="GithubFeaturev1Result">
        <include refid="selectGithubFeaturev1Vo"/>
        <where>  
            <if test="projectId != null  and projectId != ''"> and project_id = #{projectId}</if>
            <if test="featuresName != null  and featuresName != ''"> and features_name like concat('%', #{featuresName}, '%')</if>
            <if test="projectName != null  and projectName != ''"> and project_name like concat('%', #{projectName}, '%')</if>
            <if test="featuresLanuage != null  and featuresLanuage != ''"> and features_lanuage = #{featuresLanuage}</if>
            <if test="featuresFrontEnd != null "> and features_front_end = #{featuresFrontEnd}</if>
            <if test="featuresScore != null "> and features_score = #{featuresScore}</if>
